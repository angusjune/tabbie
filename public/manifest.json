{
  "manifest_version": 2,
  "name": "Historie",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "icons": {
    "128": "icons/icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon-32.png",
    "default_title": "Historie",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "sessions"
  ],
  "options_ui": {
		"page": "options.html",
		"open_in_tab": false
	}
}