{
  "manifest_version": 2,
  "name": "__MSG_ext_name__",
  "short_name": "__MSG_ext_short_name__",
  "version": "0.9.1",
  "description": "__MSG_ext_description__",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon-32.png",
    "default_title": "__MSG_browser_icon_title__",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "sessions"
  ],
  "options_ui": {
		"page": "options.html",
		"open_in_tab": false
  },
  "default_locale": "en"
}